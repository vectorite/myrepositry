			<field name="chain_select-enabled-first_option" type="text" default="" 
				label="CE_CF_SQL_CHAIN_SELECT_LINKED_FIRST_OPTION_LABEL" 
				description="CE_CF_SQL_CHAIN_SELECT_LINKED_FIRST_OPTION_DESC" />
			
		</fieldset>
		
