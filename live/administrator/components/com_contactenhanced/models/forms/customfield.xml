				description="CE_CF_RADIOBUTTON_CHAIN_SELECT_FIELD_TO_UPDATE_DESC" 
						query="SELECT id, name  FROM #__ce_cf WHERE type = 'sql' AND published=1"
						key_field="id" value_field="name" />
			
			<field name="chain_select-enabled-title2" subgroup="chain_select-enabled" type="title" 
				label="CE_CF_RADIOBUTTON_CHAIN_SELECT_TITLE_LABEL" 
					description="CE_CF_RADIOBUTTON_CHAIN_SELECT_TITLE_DESC" />
					
			<field name="chain_select-enabled-first_option" type="text" default="" 
				label="CE_CF_RADIOBUTTON_CHAIN_SELECT_FIRST_OPTION_LABEL" 
						description="CE_CF_RADIOBUTTON_CHAIN_SELECT_FIRST_OPTION_DESC" />
				
		</fieldset>
		
