				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
			</field>
			<field name="display_type-checkbox-select_all_button" type="radio" default="0" 
				label="CE_CF_CHECKBOX_SELECT_ALL_BUTTON_LABEL" 
