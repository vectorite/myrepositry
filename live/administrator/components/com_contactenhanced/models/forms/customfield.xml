				description="CE_CF_RECIPIENT_USE_CONTACT_EMAILS_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			
			<field name="display_type-checkbox-number_of_columns" type="list" default="1" 
