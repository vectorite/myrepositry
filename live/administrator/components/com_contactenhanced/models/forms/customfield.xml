<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="form">
		<field name="id" type="text" label="JGLOBAL_FIELD_ID_LABEL"
			description="JGLOBAL_FIELD_ID_DESC" size="10" default="0"
			required="true" readonly="true" class="readonly" />

		<field name="name" type="text"
			label="CE_CF_NAME_LABEL" description="CE_CF_NAME_DESC"
			class="inputbox" size="30" required="true" />

		<field id="published" name="published" type="list"
			label="JSTATUS" description="JFIELD_PUBLISHED_DESC"
			class="inputbox" size="1" default="1">
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="-2">JTRASHED</option>
		</field>

		<field name="catid" type="category" extension="com_contactenhanced"
			label="JCATEGORY" description="JFIELD_CATEGORY_DESC"
			class="inputbox" required="true"
		>
			<option value="0">JALL</option>
		</field>

		<field name="access" type="accesslevel" label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC" class="inputbox" size="1" />
		<field name="required" type="radio" default="0" label="Required" 
			description="Set to Yes to make this field mandatory" >
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field>
		<field name="type" type="groupedlist"
				label="CE_CF_FIELD_TYPE" description="CE_CF_FIELD_TYPE_DESC"
				onchange="updateInputs(this.value);" class="inputbox">
				
				<option value="text">CE_CF_TYPE_TEXT</option>
				<option value="autocomplete">CE_CF_TYPE_AUTOCOMPLETE</option>
				<option value="button">CE_CF_TYPE_BUTTON</option>
				<option value="checkbox">CE_CF_TYPE_CHECKBOX</option>
				<option value="date">CE_CF_TYPE_DATE</option>
				<option value="daterange">CE_CF_TYPE_DATERANGE</option>
				<option value="freetext">CE_CF_TYPE_FREETEXT</option>
				<option value="hidden">CE_CF_TYPE_HIDDEN_INPUT</option>
				<option value="multiplefiles">CE_CF_TYPE_MULTIPLE_FILES</option>
				<option value="multitext">CE_CF_TYPE_MULTITEXT</option>
				<option value="number">CE_CF_TYPE_NUMBER</option>
				<option value="numberrange">CE_CF_TYPE_NUMBER_RANGE</option>
				<option value="pagination">CE_CF_TYPE_PAGINATION</option>
				<option value="radiobutton">CE_CF_TYPE_RADIOBOX</option>
				<option value="selectlist">CE_CF_TYPE_SELECTLIST</option>
				<option value="sql">CE_CF_TYPE_SQL</option>
				<option value="wysiwyg">CE_CF_TYPE_WYSIWYG</option>
				
				<group label="CE_CF_GROUP_ONLY_ONE_PER_CATEGORY"> </group>
					<option value="name">CE_CF_TYPE_NAME</option>
					<option value="surname">CE_CF_TYPE_SURNAME</option>
					<option value="email">CE_CF_TYPE_EMAIL</option>
					<!-- option value="email_verify">CE_CF_TYPE_EMAIL_VERIFY</option -->
					<option value="subject">CE_CF_TYPE_SUBJECT</option>
					<option value="recipient">CE_CF_TYPE_RECIPIENT</option>
					<option value="username">CE_CF_TYPE_USERNAME</option>
					<option value="password">CE_CF_TYPE_PASSWORD</option>
					
				<group label="CE_CF_GROUP_NEWSLETTERS"> </group>
					<option value="acymailing">CE_CF_TYPE_ACYMAILING</option>
					<option value="campaignmonitor">CE_CF_TYPE_CAMPAIGNMONITOR</option>
					<option value="mailchimp">CE_CF_TYPE_MAILCHIMP</option>
					
				<group label="CE_CF_GROUP_OTHER_INTEGRATIONS"> </group>	
					<option value="gmapsaddress">CE_CF_TYPE_GMAPSADDRESS</option>
					
				<group label="CE_CF_GROUP_INSERT_CODE"> </group>	
					<option value="css">CE_CF_TYPE_CSS</option>
					<option value="js">CE_CF_TYPE_JS</option>
					<option value="php">CE_CF_TYPE_PHP</option>
				
					
			</field>

		<field name="language" type="contentlanguage" label="JFIELD_LANGUAGE_LABEL"
			description="COM_CONTACTENHANCED_FIELD_LANGUAGE_DESC" class="inputbox">
			<option value="*">JALL</option>
		</field>
		<field name="ordering" type="ordering" class="inputbox" default="999"
			label="JFIELD_ORDERING_LABEL" description="JFIELD_ORDERING_DESC"
			table="#__ce_cf" />
		<field name="tooltip" type="textarea"
			label="CE_CF_TOOLTIP"
			description="CE_CF_TOOLTIP_DESC"
			class="inputbox" rows="6" cols="50" />
			
		<field name="attributes" type="textarea"
			label="CE_CF_ATTRIBUTES"
			description="CE_CF_ATTRIBUTES_DESC"
			class="inputbox"
			rows="10" cols="70" />
		
	</fieldset>


	<fields name="params">
		<fieldset name="general" label="CE_CF_PARAMS_GENERAL">
			<field name="field-width" type="list" default="99" 
				label="CE_CF_PARAM_FIELD_WIDTH_LABEL" 
				description="CE_CF_PARAM_FIELD_WIDTH_DESC">
				<option value="100">CE_CF_PARAM_FIELD_WIDTH_OPT_100_COLUMN</option>
				<option value="50">CE_CF_PARAM_FIELD_WIDTH_OPT_50_COLUMNS</option>
				<option value="33">CE_CF_PARAM_FIELD_WIDTH_OPT_33_COLUMNS</option>
				<option value="25">CE_CF_PARAM_FIELD_WIDTH_OPT_25_COLUMNS</option>
				<option value="20">CE_CF_PARAM_FIELD_WIDTH_OPT_20_COLUMNS</option>
				<option value="15">CE_CF_PARAM_FIELD_WIDTH_OPT_15_COLUMNS</option>
				<option value="75">CE_CF_PARAM_FIELD_WIDTH_OPT_75_COLUMNS</option>
				<option value="66">CE_CF_PARAM_FIELD_WIDTH_OPT_67_COLUMNS</option>
			</field>
			<field name="hide_field" type="radio" default="0" 
				label="CE_CF_PARAM_HIDE_FIELD_LABEL" 
				description="CE_CF_PARAM_HIDE_FIELD_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="hide_field_label" type="list" default="0" 
				label="CE_CF_PARAM_HIDE_FIELDS_LABEL_LABEL" 
				description="CE_CF_PARAM_HIDE_FIELDS_LABEL_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
				<option value="overtext">CE_CF_PARAM_SHOW_LABEL_OVER_FIELD</option>
			</field>
			<field name="tooltip_behavior" type="radio" default="mouseover" 
				label="CE_CF_TOOLTIP_BEHAVIOR_LABEL" 
				description="CE_CF_TOOLTIP_BEHAVIOR_DESC" >
				<option value="mouseover">CE_CF_TOOLTIP_BEHAVIOR_ON_MOUSE_OVER</option>
				<option value="inline">CE_CONFIG_INLINE</option>
			</field>
		</fieldset>

		<fieldset name="autocomplete" label="CE_CF_PARAMS_autocomplete">
			<field name="autocomplete_allow_multiple_lines" type="radio" default="0" 
			label="CE_CF_SQL_MULTIPLE_LINES_LABEL" 
			description="CE_CF_SQL_MULTIPLE_LINES_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="autocomplete_show_quantity" type="radio" default="0" 
			label="CE_CF_SQL_SHOW_QUANTATY_LABEL" 
			description="CE_CF_SQL_SHOW_QUANTATY_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="autocomplete_quantity_validation" type="list" default="0" 
				label="CE_CF_SQL_QUANTITY_VALIDATION_LABEL" 
				description="CE_CF_SQL_QUANTITY_VALIDATION_DESC" >
				<option value="0"					>CE_CF_TEXT_VALIDATION_OPT_NO_VALIDATION</option>
				<option value="validate-alpha"		>CE_CF_TEXT_VALIDATION_OPT_ALPHA</option>
				<option value="validate-alphanum"	>CE_CF_TEXT_VALIDATION_OPT_ALPHANUMERIC</option>
				<option value="validate-digits"		>CE_CF_TEXT_VALIDATION_OPT_DIGITS</option>
				<option value="validate-integer"	>CE_CF_TEXT_VALIDATION_OPT_INTEGER</option>
				<option value="validate-numeric"	>CE_CF_TEXT_VALIDATION_OPT_NUMERIC</option>
			</field>
			<field name="autocomplete_show_heading" type="radio" default="0" 
				label="CE_CF_SQL_SHOW_FIELD_HEADINGS_LABEL" 
				description="CE_CF_SQL_SHOW_FIELD_HEADINGS_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			
			
		</fieldset>
		
		<!-- CUSTOM FIELDS SPECIFIC PARAMETERS -->
		<fieldset name="gmapsaddress" label="CE_CF_PARAMS_GMAPSADDRESS">
			<field name="gmaps_linkToGoogleEarth" type="list" default=""
					label="CE_GMAPS_LINK_OUTPUT_LABEL"
					description="CE_GMAPS_LINK_OUTPUT_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="gmaps">CE_GMAPS_LINK_OUTPUT_OPT_GOOGLE_MAPS</option>
				<option value="gearth">CE_GMAPS_LINK_OUTPUT_OPT_GOOGLE_EARTH</option>
			</field>
			<field name="gmap_mapTypeControl" type="list" default=""
					label="CE_GMAPS_MAPTYPE_CONTROL"
					description="CE_GMAPS_MAPTYPE_CONTROL_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="0">JHIDE</option>
				<option value="1">JSHOW</option>
			</field>
			<field name="gmaps_MapTypeId" type="list" default=""
					label="CE_GMAPS_MAPTYPE"
					description="CE_GMAPS_MAPTYPE_ROADMAP_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="ROADMAP">CE_GMAPS_MAPTYPE_ROADMAP</option>
				<option value="SATELLITE">CE_GMAPS_MAPTYPE_SATELLITE</option>
				<option value="HYBRID">CE_GMAPS_MAPTYPE_HYBRID</option>
				<option value="TERRAIN">CE_GMAPS_MAPTYPE_TERRAIN</option>
			</field>
			<field name="gmaps_showCoordinates" type="list" default=""
					label="CE_GMAPS_SHOW_COORDINATES"
					description="CE_GMAPS_SHOW_COORDINATES_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="0">JHIDE</option>
				<option value="1">JSHOW</option>
			</field>
			<field name="gmap_scrollWheel" type="list" default=""
					label="CE_GMAPS_SCROLLWHEEL"
					description="CE_GMAPS_SCROLLWHEEL_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="1">CE_TRUE</option>
				<option value="0">CE_FALSE</option>
			</field>
			<field name="gmaps_icon" type="imagelist" 
					directory="/components/com_contactenhanced/assets/images/gmaps/marker" hide_none="1" 
					default="" label="CE_GMAPS_MARKER_ICON" 
					description="CE_GMAPS_MARKER_ICON_DESC" />
			<field name="gmaps_icon_shadow" type="imagelist" 
					directory="/components/com_contactenhanced/assets/images/gmaps/shadow" hide_none="1" 
					default="" label="CE_GMAPS_MARKER_ICON_SHADOW" 
					description="CE_GMAPS_MARKER_ICON_SHADOW_DESC" />
			
			
			<field name="gmap_infoWindowDisplay" type="list" default=""
					label="CE_GMAPS_INFO_WINDOW_DISPLAY"
					description="CE_GMAPS_INFO_WINDOW_DISPLAY_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="alwaysOn">CE_GMAPS_INFO_WINDOW_DISPLAY_ALWAYS_ON</option>
				<option value="onClick">CE_GMAPS_INFO_WINDOW_DISPLAY_ON_CLICK</option>
				<option value="hide">JHIDE</option>
			</field>
			<field name="gmap_infoWindowDisplay" type="list" default=""
					label="CE_GMAPS_INFO_WINDOW_DISPLAY"
					description="CE_GMAPS_INFO_WINDOW_DISPLAY_DESC">
				<option value="">JGLOBAL_USE_GLOBAL</option>
				<option value="alwaysOn">CE_GMAPS_INFO_WINDOW_DISPLAY_ALWAYS_ON</option>
				<option value="onClick">CE_GMAPS_INFO_WINDOW_DISPLAY_ON_CLICK</option>
				<option value="hide">JHIDE</option>
			</field>
			<field name="gmap_infoWindowContent" type="textarea" default=" "
					label="CE_GMAPS_INFO_WINDOW_CONTENT"
					description="CE_GMAPS_INFO_WINDOW_CONTENT_DESC"
					class="inputbox" rows="3" cols="30" />
			
			<field name="gmaps_lat" type="text" default=""
				label="CE_MAPS_LATITUDE_LABEL"
				description="CE_MAPS_LATITUDE_DESC"
				 />
			
			<field name="gmaps_lng" type="text" default=""
				label="CE_MAPS_LONGITUDE_LABEL"
				description="CE_MAPS_LONGITUDE_DESC"
				 />
			
			<field name="gmaps_zoom" type="list" default="12"
				label="CE_MAPS_ZOOM_LABEL"
				description="CE_MAPS_ZOOM_DESC">
				<option value="1">J1</option>
				<option value="2">J2</option>
				<option value="3">J3</option>
				<option value="4">J4</option>
				<option value="5">J5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="10">J10</option>
				<option value="11">11</option>
				<option value="12">12</option>
				<option value="13">13</option>
				<option value="14">14</option>
				<option value="15">J5</option>
				<option value="16">16</option>
				<option value="17">17</option>
				<option value="18">18</option>
				<option value="19">19</option>
				<option value="20">J20</option>
			</field>
			
		</fieldset>
		<fieldset name="checkbox" label="CE_CF_PARAMS_CHECKBOX">
			<field name="checkbox_first_selected" type="radio" default="0" 
				label="CE_CF_CHECKBOX_FIRST_OPTION_SELECTED_LABEL" 
				description="CE_CF_CHECKBOX_FIRST_OPTION_SELECTED_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="number_of_columns" type="list" default="1" 
				label="CE_CF_CHECKBOX_NUMBER_OF_COLUMNS_LABEL" 
				description="CE_CF_CHECKBOX_NUMBER_OF_COLUMNS_DESC" >
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
			</field>
			<field name="select_all_button" type="radio" default="0" 
				label="CE_CF_CHECKBOX_SELECT_ALL_BUTTON_LABEL" 
				description="CE_CF_CHECKBOX_SELECT_ALL_BUTTON_DESC" >
				<option value="1">Show</option>
				<option value="0">Hide</option>
			</field>
		</fieldset>
		
		
		<fieldset name="freetext" label="CE_CF_PARAMS_FREETEXT">
			<field name="parse_content_plugins" type="radio" default="0" 
				label="CE_CF_FREETEXT_PROCESS_PLUGINS_LABEL" 
				description="CE_CF_FREETEXT_PROCESS_PLUGINS_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
		</fieldset>
		
		<fieldset name="date" label="CE_CF_PARAMS_DATE">
			<field name="date-format" type="list" default="%d-%m-%Y" 
				label="CE_CF_DATE_FORMAT_LABEL"
				 description="CE_CF_DATE_FORMAT_DESC">
				 <option value="%d-%m-%Y">CE_CF_DATE_FORMAT_OPT_D_M_Y</option>
				 <option value="%m-%d-%Y">CE_CF_DATE_FORMAT_OPT_M_D_Y</option>
				 <option value="%Y-%m-%d">CE_CF_DATE_FORMAT_OPT_Y_M_D</option>
				 <option value="%d-%b-%Y">CE_CF_DATE_FORMAT_OPT_D_MON_Y</option>
				 <option value="%b-%d-%Y">CE_CF_DATE_FORMAT_OPT_MON_D_Y</option>
			</field>
		</fieldset>
		<fieldset name="daterange" label="CE_CF_PARAMS_DATERANGE">
			<field name="date-format" type="list" default="%d-%m-%Y" 
				label="CE_CF_DATE_FORMAT_LABEL"
				 description="CE_CF_DATE_FORMAT_DESC">
				 <option value="%d-%m-%Y">CE_CF_DATE_FORMAT_OPT_D_M_Y</option>
				 <option value="%m-%d-%Y">CE_CF_DATE_FORMAT_OPT_M_D_Y</option>
				 <option value="%Y-%m-%d">CE_CF_DATE_FORMAT_OPT_Y_M_D</option>
				 <option value="%d-%b-%Y">CE_CF_DATE_FORMAT_OPT_D_MON_Y</option>
				 <option value="%b-%d-%Y">CE_CF_DATE_FORMAT_OPT_MON_D_Y</option>
			</field>
		</fieldset>
		
		<fieldset name="multiplefiles" label="CE_CF_PARAMS_MULTIPLEFILES">
			<field name="mf_filter_type" type="radio" default="whitelist" 
				label="CE_CF_MULTIPLEFILES_RESTRICTION_TYPE_LABEL" 
				description="CE_CF_MULTIPLEFILES_RESTRICTION_TYPE_DESC">
				<option value="blacklist">CE_CF_MULTIPLEFILES_RESTRICTION_TYPE_OPT_BLACKLIST</option>
				<option value="whitelist">CE_CF_MULTIPLEFILES_RESTRICTION_TYPE_OPT_WHITELIST</option>
			</field>
			<field name="mf_filter_file_extensions" type="text" size="" 
				default="doc,docx,ppt,xls,pdf,jpg,gif,png,zip" 
				label="CE_CF_MULTIPLEFILES_FILE_EXTENSIONS_LABEL" 
				description="CE_CF_MULTIPLEFILES_FILE_EXTENSIONS_DESC" />
			<field name="max_file_size" type="text" default="300" 
				label="CE_CF_MULTIPLEFILES_MAX_FILE_SIZE_LABEL" 
				description="CE_CF_MULTIPLEFILES_MAX_FILE_SIZE_DESC" />
			<field name="mf_number_of_files" type="text" default="3" 
				label="CE_CF_MULTIPLEFILES_NUMBER_OF_FILES_LABEL" 
				description="CE_CF_MULTIPLEFILES_NUMBER_OF_FILES_DESC" />
		</fieldset>
		
		<fieldset name="radiobutton" label="CE_CF_PARAMS_RADIOBUTTON">
			<field name="radiobutton_first_selected" type="radio" default="0" 
				label="CE_CF_RADIOBUTTON_FIRST_OPTION_SELECTED_LABEL" 
				description="CE_CF_RADIOBUTTON_FIRST_OPTION_SELECTED_DESC" >
				<option value="0">JNO</option>
				<option value="1">JYES</option>
			</field>
			<field name="number_of_columns" type="list" default="1" 
				label="CE_CF_RADIOBUTTON_NUMBER_OF_COLUMNS_LABEL" 
				description="CE_CF_RADIOBUTTON_NUMBER_OF_COLUMNS_DESC" >
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
			</field>
			<field name="chain_select" type="switch" default="0" 
				label="CE_CF_RADIOBUTTON_CHAIN_SELECT_LABEL" 
				description="CE_CF_RADIOBUTTON_CHAIN_SELECT_DESC" >
				<option value="0">JDISABLED</option>
				<option value="enabled">JENABLED</option>
